<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.shaobin0604.webviewtest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="io.github.shaobin0604.webviewtest.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="io.github.shaobin0604.webviewtest.WithoutWebViewClientActivity" 
            android:label="without WebViewClient"/>
        <activity android:name="io.github.shaobin0604.webviewtest.WithDefaultWebViewClientActivity"
            android:label="with default WebViewClient"/>
        <activity android:name="io.github.shaobin0604.webviewtest.WithWebViewClientShouldOverrideUrlLoadingActivity"
            android:label="with WebViewClient shouldOverrideUrlLoading"/>
    </application>

</manifest>